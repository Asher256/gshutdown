#: ../src/args.c:48
msgid "Launch the action after a certain delay : HH:MM"
msgstr "Executa l'acció després d'un retard: HH:MM"

#: ../src/args.c:49
msgid "Launch the action at : HH:MM YYYY/MM/DD"
msgstr "Executa l'acció a: HH:MM DD/MM/YYYY"

#: ../src/args.c:50
msgid "Launch the action now."
msgstr "Executa l'acció ara."

#: ../src/args.c:51
msgid "Available actions are : \"shutdown\", \"reboot\" or \"logout\""
msgstr "Accions disponibles són: \"shutdown\", \"reboot\" o \"logout\""

#: ../src/args.c:52
msgid "Minimize the window at startup."
msgstr "Minimitza aquesta finestra a l'inici"

#: ../src/args.c:64
#, c-format
msgid "Error when parsing the arguments...\n"
msgstr "Hi ha hagut un error al tractar els arguments...\n"

#: ../src/args.c:81
#, c-format
msgid ""
"You cannot call --launch-after-delay, --launch-at or --launch-now at the "
"same time.\n"
msgstr ""
"No pots cridar --launch-after-delay, --launch-at o --launch-now al mateix "
"temps.\n"

#: ../src/args.c:106
#, c-format
msgid ""
"The authorized values in --action option are : shutdown, reboot or logout\n"
msgstr ""
"Els valors autoritzats per l'opció --action són: shutdown, reboot or logout\n"

#: ../src/args.c:131
#, c-format
msgid "Error on parsing the content of %s : \"%s\".\n"
