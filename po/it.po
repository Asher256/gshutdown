#: ../src/args.c:48
msgid "Launch the action after a certain delay : HH:MM"
msgstr "Eseguire l'azione con un ritardo di : HH:MM"

#: ../src/args.c:49
msgid "Launch the action at : HH:MM YYYY/MM/DD"
msgstr "Eseguire l'azione alle :HH:MM YYYY/MM/DD"

#: ../src/args.c:50
msgid "Launch the action now."
msgstr "Esegui l'azione ora."

#: ../src/args.c:51
msgid "Available actions are : \"shutdown\", \"reboot\" or \"logout\""
msgstr "Le azioni disponibili sono : \"shutdown\", \"reboot\" or \"logout\""

#: ../src/args.c:52
msgid "Minimize the window at startup."
msgstr "Minimizza la finestra all'avvio."

