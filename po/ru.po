#: ../src/args.c:48
msgid "Launch the action after a certain delay : HH:MM"
msgstr "Запустить действие после определенной задержки: ЧЧ:ММ"

#: ../src/args.c:49
msgid "Launch the action at : HH:MM YYYY/MM/DD"
msgstr "Запустить команду в ЧЧ:ММ ГГГГ/ММ/ДД"

#: ../src/args.c:50
msgid "Launch the action now."
msgstr "Запустить событие немедленно."

#: ../src/args.c:51
msgid "Available actions are : \"shutdown\", \"reboot\" or \"logout\""
msgstr "Доступные действия: \"shutdown\", \"reboot\" или \"logout\""

